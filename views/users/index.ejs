<!DOCTYPE html>
  <html lang="en">
    <body>
      <%- include('../partials/nav')%>
    <div class="row">
      <div id="users" class="col s12">
        <% users.forEach(function(user) { %>
              <div>
                  <i class="small material-icons">perm_identity</i>
                  <span class="user-name">
                    <a href="/users/<%= user._id%>"><%= user.name %></a>
                  </span>
                  <table>
                    <thead>
                      <th>Restaurants</th>
                      <th>Location</th>
                    </thead>
                    <tbody>
                      <% user.restaurants.forEach(function(res){ %>
                        <tr>
                        <td> <%=res.resName%></td>
                        <td> <%=res.city%></td>
                      </tr>
                      <%})%>
                    </tbody>
                  </table>
                </span>
          </div>
          <% }) %>
      </div>
      </div>
  </body>
</html>
